NAME=school
CC=gcc
SRC=main.c school.c
MAIN_OBJ=main.o
SCHOOL_OBJ=school.o
OBJ=$(MAIN_OBJ) $(SCHOOL_OBJ)
.PHONEY: all
all: $(NAME)
$(NAME): $(OBJ)
	@-rm -f $(NAME)
	$(CC) $(OBJ) -o $(NAME)
$(MAIN_OBJ): main.c m.h
	$(CC) -c -o $(MAIN_OBJ) main.c
$(SCHOOL_OBJ): school.c
	$(CC) -c -o $(SCHOOL_OBJ) school.c
